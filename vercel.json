{
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.6.2"
        }
    },
    "routes": [
        {
            "src": "/api/calendário.php",
            "dest": "/api/calendário.php"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}
